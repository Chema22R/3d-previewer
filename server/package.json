    "dependencies": {
        "@sentry/node": "^5.13.1",
        "body-parser": "^1.19.0",
        "busboy": "^0.3.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "logdna": "^3.5.0",
        "mongoose": "^5.9.2"
    },
    "devDependencies": {
